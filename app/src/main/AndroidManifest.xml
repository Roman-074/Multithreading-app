<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="hedgehog.tech.multithreadingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Multithreadingapp">


        <activity android:name=".main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".coroutines.Coroutines0"/>
        <activity android:name=".coroutines.Coroutines1"/>


        <activity android:name=".coroutines.Coroutines7"/>
        <activity android:name=".coroutines.Coroutines3"/>
        <activity android:name=".coroutines.Coroutines4"/>
        <activity android:name=".coroutines.Coroutines5"/>

        <activity android:name=".livedata.LiveData0"/>

        <activity android:name=".flow.Flow0"/>
        <activity android:name=".flow.Flow1"/>

        <activity android:name=".rxjava.RxJava"/>


    </application>

</manifest>